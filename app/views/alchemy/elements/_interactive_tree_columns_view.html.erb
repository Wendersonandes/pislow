<%- cache(element) do -%>
  <%= element_view_for(element) do |el| -%>
    <div class="headline1">
      <%= el.render :headline1 %>
    </div>
    <div class="text1">
      <%= el.render :text1 %>
    </div>
    <div class="picture1">
      <%= el.render :picture1 %>
    </div>
    <div class="headline2">
      <%= el.render :headline2 %>
    </div>
    <div class="text2">
      <%= el.render :text2 %>
    </div>
    <div class="picture2">
      <%= el.render :picture2 %>
    </div>
    <div class="headline3">
      <%= el.render :headline3 %>
    </div>
    <div class="text3">
      <%= el.render :text3 %>
    </div>
    <div class="picture3">
      <%= el.render :picture3 %>
    </div>
  <%- end -%>
<%- end -%>
